{
  "name": "app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "clean": "git clean -xdf .next .turbo node_modules",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "start": "expo start",
    "dev": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint && eslint .",
    "typecheck": "tsc --noEmit",
    "supabase": "supabase",
    "supabase:start": "supabase status || supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:status": "supabase status",
    "supabase:test": "supabase db test",
    "supabase:db:lint": "supabase db lint",
    "supabase:deploy": "supabase link --project-ref $SUPABASE_PROJECT_REF && supabase db push",
    "supabase:typegen": "pnpm run supabase:typegen:packages && pnpm run supabase:typegen:app",
    "supabase:typegen:packages": "supabase gen types typescript --local > ../../packages/supabase/src/database.types.ts",
    "supabase:typegen:app": "supabase gen types typescript --local > ./database.types.ts",
    "supabase:db:dump:local": "supabase db dump --local --data-only"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@kit/account": "workspace:*",
    "@kit/auth": "workspace:*",
    "@kit/supabase": "workspace:^",
    "@kit/ui": "workspace:*",
    "@react-navigation/native": "^7.0.14",
    "@tanstack/react-query": "^5.62.16",
    "expo": "~52.0.23",
    "expo-constants": "~17.0.3",
    "expo-font": "~13.0.2",
    "expo-linking": "~7.0.3",
    "expo-router": "~4.0.15",
    "expo-splash-screen": "~0.29.18",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.6",
    "expo-web-browser": "~14.0.1",
    "lucide-react-native": "^0.469.0",
    "metro-cache": "^0.81.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "^0.76.5",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.6",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "4.4.0",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@kit/eslint-config": "workspace:*",
    "@kit/prettier-config": "workspace:*",
    "@kit/tailwind-config": "workspace:*",
    "@kit/typescript-config": "workspace:*",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.11",
    "@types/react-test-renderer": "18.0.7",
    "eslint-config-expo": "^8.0.1",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "nativewind": "4.1.23",
    "react-test-renderer": "18.2.0",
    "supabase": "2.2.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  },
  "prettier": "@kit/prettier-config",
  "private": true
}
